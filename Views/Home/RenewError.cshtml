@model RenewalException
@{ ViewData["Title"] = $"Cert Creation - {Model.SecretNamespace}/{Model.SecretName}"; }

<h1>Certificate Renewal Results: @Model.SecretNamespace/@Model.SecretName</h1>

<p>
    <a class="btn btn-success" asp-action="Index">Return To Main Page</a>
</p>

<h2>Logs</h2>
<ul>
    @foreach (var line in Model.Logs)
    {
<li>@line</li>
}
</ul>

<h2>Exception</h2>
<pre>@Model.GetType().Name</pre>
<pre>@Model.Message</pre>
<pre>@Model.StackTrace</pre>

<h2>Inner Exception</h2>
<pre>@Model.InnerException.GetType().Name</pre>
<pre>@Model.InnerException.Message</pre>
<pre>@Model.InnerException.StackTrace</pre>
