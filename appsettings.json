{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Deploy": {
    "Namespace": "kcert",
    "SecretName": "kcert",
    "ServiceName": "kcert",
    "ServicePort": "http",
    "ManagedNamespaces": [ "*" ]
  },
  "ACME": {
    "AcmeWaitTimeSeconds": 10,
    "AcmeNumRetries": 5,
    "RenewalCheckTimeHours": 24,
    "RenewalExpirationRenewalDays": 30,
  }
}
